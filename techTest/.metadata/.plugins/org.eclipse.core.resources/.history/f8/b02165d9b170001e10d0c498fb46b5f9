package com.fabianpalma.techtest.services;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.fabianpalma.techtest.repositories.EscritosRepository;

@Service
public class EstadosService {
	@Autowired
	private final EscritosRepository escritoRepository;
	
	public EstadosService(EscritosRepository escritoRepository) {
		this.escritoRepository = escritoRepository;
	}
}
